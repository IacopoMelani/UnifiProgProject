package javamm;

@SuppressWarnings("all")
public class ToUpperCase {
  public static char[] upperCase(char[] a) {
    int len = a.length;
    for (int i = 0; (i < (len - 1)); i++) {
      {
        int charToInt = a[i];
        if ((charToInt < 96)) {
          charToInt = (charToInt + 32);
        }
      }
    }
    return a;
  }
  
  public static void main(String[] args) {
  }
}
