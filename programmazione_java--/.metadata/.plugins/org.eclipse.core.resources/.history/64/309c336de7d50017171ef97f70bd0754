char[] upperCase(char[] a) {
	int len = a.length;
	for (int i = 0; i < len - 1; i++) {
		int charToInt = a[i];
		if (charToInt > 96) {
			System.out.println("diocam");
			charToInt = charToInt + 32;
		}
		a[i] = (char)charToInt;
	}
	return a;
}

void printArray(char[] array) {
	int n = array.length;
	for (int i = 0; i < n; i++) {
		System.out.print(array[i] + " ");
	}
}


char[] a = {'c','i','a','o'};


printArray(upperCase(a));