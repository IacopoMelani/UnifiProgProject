boolean strangePartition(int[] a) {
	return false;
}

int genBinary(int[] a, int[] cpA, int b)

boolean odd(int n){
	boolean ris = false;
	ris = n % 2 != 0;
	return ris;
}


boolean multipleOfTen(int n){
	boolean ris = false;
	ris = n % 10 == 0;
	return ris;
}


int sumArray(int[] a, int[] binary, int c){
	int sum = 0;
	for(int i = 0; i < a.length; i++){
		if(binary[i] == c){
			sum = sum + a[i];
		}
	}
	return sum;
}

int[] copyArray(int[] a){
	int[] b = new int[a.length];
	for(int i = 0; i < a.length; i++){
		b[i] = a[i];
	}
	return b;
}