package javamm;

@SuppressWarnings("all")
public class HarmonicSum {
  public static int harmonicSum(int x) {
    float sum = 0;
    while ((sum < x)) {
    }
    return 0;
  }
  
  public static float harmonic(int k) {
    float i = 1;
    float sum = 0;
    do {
      {
        sum = (sum + (1 / i));
        i++;
      }
    } while((i != k));
    return sum;
  }
  
  public static void main(String[] args) {
    System.out.println(HarmonicSum.harmonic(10));
  }
}
