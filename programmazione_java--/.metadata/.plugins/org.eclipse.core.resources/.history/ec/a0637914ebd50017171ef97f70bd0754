package javamm;

@SuppressWarnings("all")
public class PalindromeArray {
  public static boolean palindrome(int[] a) {
    boolean result = true;
    int len = a.length;
    int top = (len - 1);
    for (int down = 0; (down < len); down++) {
      boolean _tripleNotEquals = (a[down] != a[top]);
      if (_tripleNotEquals) {
        return false;
      }
    }
    return true;
  }
  
  public static void main(String[] args) {
  }
}
