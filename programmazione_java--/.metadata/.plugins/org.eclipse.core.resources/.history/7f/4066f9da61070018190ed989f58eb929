package javamm;

@SuppressWarnings("all")
public class Armstrong {
  public static boolean armstrong(int n) {
    return false;
  }
  
  public static int contDigit(int n) {
    int digit = 0;
    while ((n != 0)) {
      {
        n = (n / 10);
        digit = (digit + 1);
      }
    }
    return digit;
  }
  
  public static void main(String[] args) {
    int n = 100;
    String _plus = (Integer.valueOf(n) + " ");
    int _contDigit = Armstrong.contDigit(n);
    String _plus_1 = (_plus + Integer.valueOf(_contDigit));
    System.out.println(_plus_1);
  }
}
