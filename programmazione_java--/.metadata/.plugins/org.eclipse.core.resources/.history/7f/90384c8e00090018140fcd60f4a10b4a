package javamm;

@SuppressWarnings("all")
public class KSubsetSum {
  public static boolean kSubsetSum(int[] a, int k, int s) {
    boolean ris = KSubsetSum.genBinary(a, a, a.length, s, k);
    return ris;
  }
  
  public static boolean genBinary(int[] a, int[] cpa, int b, int s, int r) {
    if (((b == 0) && KSubsetSum.kSumArray(cpa, a, s))) {
      return true;
    } else {
      if ((r < b)) {
        a[(b - 1)] = 0;
        KSubsetSum.genBinary(a, cpa, (b - 1), s, r);
      } else {
        if ((r > 0)) {
          a[(b - 1)] = 1;
          KSubsetSum.genBinary(a, cpa, (b - 1), s, (r - 1));
        }
      }
      return false;
    }
  }
  
  public static boolean kSumArray(int[] a, int[] binary, int s) {
    boolean ris = false;
    int sum = 0;
    for (int i = 0; (i < a.length); i++) {
      boolean _tripleEquals = (binary[i] == 1);
      if (_tripleEquals) {
        int _plus = (sum + a[i]);
        sum = _plus;
      }
    }
    if ((sum == s)) {
      ris = true;
    }
    return ris;
  }
  
  public static void main(String[] args) {
    int[] a = { 1, 2, 2, 6, 7, 7, 20 };
    int[] b = { 1, 1, 1, 0, 0, 0, 1 };
  }
}
