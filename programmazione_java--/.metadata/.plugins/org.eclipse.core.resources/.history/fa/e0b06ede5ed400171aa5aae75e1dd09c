int[] arraySum(int[] a, int[] b) {
	int n = a.length;
	int[] c = new int[n];
	for (int i = 0; i < n; i++) {
		c[i] = a[i] + b[i];
	}
	return c;
}

void printArray(int[] array) {
	int n = array.length;
	for (int i = 0; i < n; i++){
		System.out.println(array[i]);
	}
}

int[] a = {3, 4, -2};
int[] b = {-1, 5, 3};

int[] c = arraySum(a, b);

System.out.println(printArray());

