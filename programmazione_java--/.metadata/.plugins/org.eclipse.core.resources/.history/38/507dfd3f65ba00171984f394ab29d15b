package javamm;

@SuppressWarnings("all")
public class Orario {
  public static void main(String[] args) {
    int millisecondi = 1000342334;
    int s = (millisecondi / 1000);
    int secondi = ((int) (s % 1000));
    int m = (s / 60);
    int minuti = ((int) (m % 60));
    int h = (m / 24);
    int ore = ((int) (h / 24));
    System.out.println(((((("h: " + Integer.valueOf(ore)) + " m: ") + Integer.valueOf(minuti)) + " s: ") + Integer.valueOf(secondi)));
  }
}
