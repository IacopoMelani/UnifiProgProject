package javamm;

@SuppressWarnings("all")
public class ToUpperCase {
  public static char[] upperCase(char[] a) {
    int len = a.length;
    for (int i = 0; (i < (len - 1)); i++) {
      {
        int charToInt = a[i];
        if ((charToInt < 96)) {
          charToInt = (charToInt + 32);
        }
        a[i] = ((char) charToInt);
      }
    }
    return a;
  }
  
  public static void main(String[] args) {
    char[] a = { 'c', 'i', 'a', 'o' };
    System.out.println();
  }
}
