package javamm;

@SuppressWarnings("all")
public class Orario {
  public static void main(String[] args) {
    long millisecondi = 1000342334;
    double s = (millisecondi / 1000);
    double secondi = (s % 1000);
    double m = (s / 60);
    double minuti = (m % 60);
    double h = (m / 24);
    double ore = (h / 24);
    System.out.println(((((("h: " + Double.valueOf(ore)) + " m: ") + Double.valueOf(minuti)) + " s: ") + Double.valueOf(secondi)));
  }
}
